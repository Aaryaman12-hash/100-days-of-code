#include <stdio.h>
int main() {
    long binary, remainder, onesComplement = 0, place = 1;

    printf("Enter a binary number: ");
    scanf("%ld", &binary);

    while (binary != 0) {
        remainder = binary % 10;
        if (remainder == 0)
            onesComplement += 1 * place;
        else
            onesComplement += 0 * place;

        binary = binary / 10;
        place *= 10;
    }

    printf("1's complement = %ld\n", onesComplement);

    return 0;
}
