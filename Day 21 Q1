#include <stdio.h>
int main() {
    int num, first, last, temp, digits = 0, pow = 1;

    printf("Enter a number: ");
    scanf("%d", &num);

    temp = num;
    last = num % 10;  
    while (temp >= 10) {
        temp = temp / 10;
        digits++;
        pow = pow * 10;
    }
    first = temp;  

    num = num % pow;      
    num = num / 10;            
    int swapped = last * pow + num * 10 + first;

    printf("Number after swapping first and last digits = %d\n", swapped);

    return 0;
}
