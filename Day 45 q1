
#include <stdio.h>
int main() {
    char str[200], ch;
    int i = 0, freq = 0;
    printf("Enter a string: ");
    scanf("%[^\n]", str);   
    printf("Enter the character to search: ");
    getchar();              
    scanf("%c", &ch);
    while (str[i] != '\0') {
        if (str[i] == ch) {
            freq++;
        }
        i++;
    }
    printf("Frequency of '%c' = %d\n", ch, freq);
    return 0;
}
